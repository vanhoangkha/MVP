{
  "version": 1,
  "paths": {
    "/usercourse": {
      "name": "/usercourse",
      "lambdaFunction": "studioUserCourse",
      "permissions": {
        "setting": "open"
      }
    },
    "/lectures": {
      "name": "/lectures",
      "lambdaFunction": "studioLectures",
      "permissions": {
        "setting": "open"
      }
    },
    "/useroverview": {
      "name": "/useroverview",
      "lambdaFunction": "userOverview",
      "permissions": {
        "setting": "open"
      }
    },
    "/courses": {
      "name": "/courses",
      "lambdaFunction": "studioCourses",
      "permissions": {
        "setting": "private",
        "auth": [
          "create",
          "read",
          "update",
          "delete"
        ]
      }
    }
  }
}