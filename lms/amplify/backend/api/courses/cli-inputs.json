      }
    },
    "/users/lectures": {
      "name": "/users/lectures",
      "lambdaFunction": "usersLectures",
      "permissions": {
        "setting": "private",
        "auth": [
          "create",
          "read",
          "update",
          "delete"
        ]
      }
